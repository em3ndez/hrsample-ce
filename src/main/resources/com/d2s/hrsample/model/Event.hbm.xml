<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping default-access="com.d2s.framework.model.persistence.hibernate.property.EntityPropertyAccessor">
  <class dynamic-update="true" table="EVENT" persister="com.d2s.framework.model.persistence.hibernate.entity.persister.EntityProxyJoinedSubclassEntityPersister" name="com.d2s.hrsample.model.Event" dynamic-insert="true">
    <id type="string" column="ID" length="36" name="id">
      <generator class="assigned"/>
    </id>
    <version column="VERSION" unsaved-value="null" name="version"/>
    <property name="text">
      <column name="TEXT" length="2048"/>
    </property>
    <property name="createTimestamp" type="timestamp">
      <column name="CREATE_TIMESTAMP"/>
    </property>
    <property name="lastUpdateTimestamp" type="timestamp">
      <column name="LAST_UPDATE_TIMESTAMP"/>
    </property>
  </class>
</hibernate-mapping>
