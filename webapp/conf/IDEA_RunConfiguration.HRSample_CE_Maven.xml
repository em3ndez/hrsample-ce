<component name="ProjectRunConfigurationManager">
  <configuration default="false" name="HRSample CE (Maven)" type="MavenRunConfiguration" factoryName="Maven" singleton="true">
    <MavenSettings>
      <option name="myGeneralSettings" />
      <option name="myRunnerSettings" />
      <option name="myRunnerParameters">
        <MavenRunnerParameters>
          <option name="profiles">
            <set />
          </option>
          <option name="goals">
            <list>
              <option value="tomcat7:run-war" />
              <option value="-pl" />
              <option value="webapp" />
            </list>
          </option>
          <option name="profilesMap">
            <map />
          </option>
          <option name="resolveToWorkspace" value="true" />
          <option name="workingDirPath" value="$PROJECT_DIR$" />
        </MavenRunnerParameters>
      </option>
    </MavenSettings>
    <method>
      <option name="Maven.BeforeRunTask" enabled="true" file="$PROJECT_DIR$/pom.xml" goal="install -pl core,webapp -P!generate-ddl-scripts -DskipTests -DdisableAttachJavadocs -DdisableAttachSources " />
    </method>
  </configuration>
</component>